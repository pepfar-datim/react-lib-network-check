  "name": "@pepfar-react-lib/network-check",
  "author": "jakub-bao",
  "module": "dist/network-check.esm.js",
  "devDependencies": {
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.612",
    "@parcel/packager-html": "^2.0.0-nightly.612",
